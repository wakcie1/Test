@{
    Layout = "~/Views/Shared/_Main.cshtml";
}
@using Common
<div class="page-header">
    <div class="pull-left">
        <ol class="breadcrumb visible-sm visible-md visible-lg">
            <li><a href="#"><i class="icon fa fa-pencil-square-o"></i>Management Report</a></li>
        </ol>
    </div>
    <div class="pull-right">

    </div>
</div>
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="panel panel-default bk-bg-white">
        <div class="panel-heading bk-bg-white">
            <h6><i class="fa fa-indent red"></i>Report</h6>
            <div class="panel-actions">
                <a href="#" class="btn-minimize"><i class="fa fa-caret-up"></i></a>
                      @*<a href="#" class="btn-closeiniframe"><i class="fa fa-times"></i></a>*@
            </div>
        </div>
        <div class="panel-body">
            <div class="col-xs-6">
                <button type="button" class="bk-margin-10 btn btn-primary btn-lg" id="btnProblemClose" data-url="@Url.Action("ProblemCloseExport", "ManagementReport")">Problem Closed Trend</button>
            </div>

        </div>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        $("#btnProblemClose").on('click', function () {
            var url = $("#btnProblemClose").attr("data-url");
            window.open(url);
        });
    });
    
</script>