

<div class="col-md-4" style="padding-bottom:10px;">
    <input type="text" id="vuserSearch" class="form-control" placeholder="member name">
    <span id="vjobnum" style="display:inline-block;" class="help-block hidden"></span>
</div>
<div class="col-md-8">
    <button id="addverification" class="btn btn-info hidden">Add <i class="fa fa-plus"></i></button>
</div>
<div class="col-md-12" style="padding-bottom:10px;">
    <table class="table table-bordered table-striped mb-none dataTable no-footer" id="datatable-editable-verification" role="grid" aria-describedby="datatable-editable_info">
        <thead>
            <tr role="row">
                <th class="sorting" tabindex="0" aria-controls="datatable-editable-verification" rowspan="1" colspan="1" style="width: 20%;">What</th>
                <th class="sorting" tabindex="0" aria-controls="datatable-editable-verification" rowspan="1" colspan="1" style="width: 20%;">Who</th>
                <th class="sorting" tabindex="0" aria-controls="datatable-editable-verification" rowspan="1" colspan="1" style="width: 15%;">Plan Date</th>
                <th class="sorting" tabindex="0" aria-controls="datatable-editable-verification" rowspan="1" colspan="1" style="width: 20%;">Actual Date</th>
                <th class="sorting" tabindex="0" aria-controls="datatable-editable-verification" rowspan="1" colspan="1" style="width: 20%;">Where</th>
                <th class="sorting" tabindex="0" aria-controls="datatable-editable-verification" rowspan="1" colspan="1" style="width: 20%;">Attachment</th>
                <th class="sorting" tabindex="0" aria-controls="datatable-editable-verification" rowspan="1" colspan="1" style="width: 20%;">Status</th>
                <th class="sorting" tabindex="0" aria-controls="datatable-editable-verification" rowspan="1" colspan="1" style="width: 20%;">Comment</th>
                <th class="sorting_disabled" rowspan="1" colspan="1" aria-label="Actions" style="width: 15%">Edit</th>
            </tr>
        </thead>
        <tbody id="verificationbody"></tbody>
    </table>
</div>
<div class="col-md-12" style="padding-bottom:10px;">
    <button type="button" class="bk-margin-right-5 btn btn-success hidden" id="approveVerification">Approve</button>
    <button type="button" class="bk-margin-right-5 btn btn-warning hidden" id="rejectVerification">Reject</button>
</div>
<div id="dialog-verification" class="modal-block modal-block-primary mfp-hide">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h2 class="panel-title">Are you sure?</h2>
        </div>
        <div class="panel-body bk-noradius">
            <div class="modal-wrapper">
                <div class="modal-icon">
                    <i class="fa fa-question-circle"></i>
                </div>
                <div class="modal-text">
                    <h4>Primary</h4>
                    <p>Are you sure that you want to delete this record?</p>
                </div>
            </div>
        </div>
        <div class="panel-footer">
            <div class="row">
                <div class="col-md-12 text-right">
                    <button id="dialogConfirm-verification" class="btn btn-primary modal-confirm">Confirm</button>
                    <button id="dialogCancel-verification" class="btn btn-default modal-dismiss">Cancel</button>
                </div>
            </div>
        </div>
    </div>
</div>
