<div class="col-md-4" style="padding-bottom:10px;">
    <input type="text" id="solvingteamuserSearch" class="form-control" placeholder="member name">
    <span id="jobnum" style="display:inline-block;" class="help-block hidden"></span>
</div>
<div class="col-md-8">
    <button id="addsolvingteam" class="btn btn-info hidden"><i class="fa fa-plus"></i> Add</button>
</div>

<div class="col-md-12" style="padding-bottom:10px;">
    <table class="table table-bordered table-striped mb-none dataTable no-footer" id="datatable-solvingteam" role="grid" aria-describedby="datatable-editable_info">
        <thead>
            <tr role="row">
                <th class="sorting" tabindex="0" aria-controls="datatable-solvingteam" rowspan="1" colspan="1" style="width: 20%;">Team Member</th>
                <th class="sorting" tabindex="0" aria-controls="datatable-solvingteam" rowspan="1" colspan="1" style="width: 15%;">Desk EXT</th>
                <th class="sorting" tabindex="0" aria-controls="datatable-solvingteam" rowspan="1" colspan="1" style="width: 20%;">Department</th>
                <th class="sorting" tabindex="0" aria-controls="datatable-solvingteam" rowspan="1" colspan="1" style="width: 15%;">Title</th>
                <th class="sorting" tabindex="0" aria-controls="datatable-solvingteam" rowspan="1" colspan="1" style="width: 15%;">IsLearder</th>
                <th class="sorting_disabled" rowspan="1" colspan="1" aria-label="Actions" style="width: 15%">Edit</th>
            </tr>
        </thead>
        <tbody id="solvingteambody"></tbody>
    </table>
</div>
<div class="col-md-12" style="padding-bottom:10px;">
    <button type="button" class="bk-margin-right-5 btn btn-success hidden" id="approveSolvingTeam">Approve</button>
    <button type="button" class="bk-margin-right-5 btn btn-warning hidden" id="rejectSolvingTeam">Reject</button>
</div>
<div id="dialog-solvingteam" class="modal-block modal-block-primary mfp-hide">
    <div class="panel panel-default">
        <div class="panel-heading">
            <h2 class="panel-title">Are you sure?</h2>
        </div>
        <div class="panel-body bk-noradius">
            <div class="modal-wrapper">
                <div class="modal-icon">
                    <i class="fa fa-question-circle"></i>
                </div>
                <div class="modal-text">
                    <h4>Primary</h4>
                    <p>Are you sure that you want to delete this record?</p>
                </div>
            </div>
        </div>
        <div class="panel-footer">
            <div class="row">
                <div class="col-md-12 text-right">
                    <button id="dialogConfirm-solvingteam" class="btn btn-primary modal-confirm">Confirm</button>
                    <button id="dialogCancel-solvingteam" class="btn btn-default modal-dismiss">Cancel</button>
                </div>
            </div>
        </div>
    </div>
</div>

