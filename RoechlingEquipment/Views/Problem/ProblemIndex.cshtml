<input type="hidden" id="workorderlisturl" value="@Url.Action("WorkOrderAutoComplete","Problem")" />
<input type="hidden" id="processlisturl" value="@Url.Action("GetProcessList","Problem", new { isNeedDefalut = false })" />
<input type="hidden" id="sourcelisturl" value="@Url.Action("GetSourceList","Problem", new { isNeedDefalut = false })" />
<input type="hidden" id="defectcodetypeurl" value="@Url.Action("GetDefectCodeTypeList","Problem",new { isNeedDefalut = false })" />
<input type="hidden" id="defectcodelisturl" value="@Url.Action("GetDefectCodeList","Problem",new { isNeedDefalut = false })" />
<input type="hidden" id="severitylisturl" value="@Url.Action("GetSeverityList","Problem")" />
<input type="hidden" id="statuslisturl" value="@Url.Action("GetStatusList","Problem")" />
<input type="hidden" id="problemunionurl" value="@Url.Action("GetProblemUnionInfo","Problem")" />
