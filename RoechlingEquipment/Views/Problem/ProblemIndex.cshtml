@{
    Layout = "~/Views/Shared/_Main.cshtml";
}
@using Common
<div class="page-header">
    <div class="pull-left">
        <ol class="breadcrumb visible-sm visible-md visible-lg">
            <li><a href="#"><i class="icon fa fa-plus-square"></i>Create</a></li>
        </ol>
    </div>
    <div class="pull-right">

    </div>
</div>

<div class="panel-body  panel-bodynopadding">
    <div class="tabs">
        <ul class="nav nav-tabs" id="tabs">
            <li>
                <a id="proInfoTab" href="#problemInfo" data-toggle="tab">Problem Info <i class="fa fa-comment-o"></i></a>
            </li>
            <li>
                <a id="proActionTab" href="#problemAction" data-toggle="tab">Action</a>
            </li>
            <li>
                <a id="proStandTab" href="#problemStand" data-toggle="tab" class="hidden">Standardlization</a>
            </li>
            <li>
                <a id="proPreiewTab" href="#problemPreview" data-toggle="tab">Preview</a>
            </li>
            <li>
                <a id="timeTrackingTab" href="#timeTracking" data-toggle="tab"  class="hidden">TimeTracking</a>
            </li>
        </ul>
        <div class="tab-content">
            <div id="problemInfo" class="tab-pane">
                @Html.Partial("ProblemInfoPage")
            </div>
            <div id="problemAction" class="tab-pane">
                @Html.Partial("ActionInfoPage")
            </div>
            <div id="problemStand" class="tab-pane">
                @Html.Partial("StandardlizationInfoPage")
            </div>
            <div id="problemPreview" class="tab-pane">
                @Html.Partial("PreviewInfoPage")
            </div>
            <div id="timeTracking" class="tab-pane">
                @Html.Partial("ProblemTimeTracking")
            </div>
        </div>
    </div>
</div>


<input type="hidden" id="proId" value="@ViewBag.ProId" /> 
<input type="hidden" id="proProcessStatus" value="0" />
<input type="hidden" id="proOwner" />
<input type="hidden" id="approveLayeredAuditflag" value="0"/>
